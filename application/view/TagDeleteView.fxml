<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.TagDeleteController">
   <children>
      <Label text="Are you sure you want to delete this tag?">
         <padding>
            <Insets left="20.0" right="20.0" top="10.0" />
         </padding>
      </Label>
      <HBox alignment="CENTER" spacing="15.0">
         <children>
            <Button fx:id="yes" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#handleYes" prefWidth="60.0" text="Yes">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="no" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#handleNo" prefWidth="60.0" text="No">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
